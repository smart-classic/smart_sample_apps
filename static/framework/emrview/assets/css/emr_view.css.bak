/* Tooltip
----------------------------------*/
.ui-tooltip {
    padding: 8px;
    position: absolute;
    z-index: 9998;
    -o-box-shadow: 2px 2px 2px #ccc;
    -moz-box-shadow: 2px 2px 2px #ccc;
    -webkit-box-shadow: 2px 2px 2px #ccc;
    box-shadow: 2px 2px 2px #ccc;
    background: rgba(255,255,255,80);
    min-width: 100px;
    max-width: 200px;
}

/* Fades and background-images don't work well together in IE6, drop the image */
* html .ui-tooltip {
    background-image: none;
}
body .ui-tooltip {
    border: 1px solid #aaa;
}

.ui-tooltip-container {
}

.ui-tooltip-content {
    overflow: auto;
    white-space: normal;
    word-wrap: break-word;
}

.ui-tooltip .ui-tooltip-close {
    display: inline-block;
    float: right;
    width: 19px;
    padding: 1px;
    height: 18px;
    margin: -2px -2px 0 2px;
}

.ui-tooltip .ui-tooltip-close:hover,
.ui-tooltip .ui-tooltip-close:focus {
    padding: 0;
}

.ui-tooltip .ui-tooltip-close span {
    display: block;
    margin: 1px;
}

.ui-helper-hidden-accessible { top: -99999999px; }